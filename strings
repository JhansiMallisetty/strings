1)package main
import "fmt"
func main() {
var wish = "hello kludone"
fmt.Printf(wish)
}


hello kludone
Program exited.

2)package main

import "fmt"

func main() {
   var greeting =  "Hello kloudne!\n"
   fmt.Printf(greeting)
   fmt.Printf("String Length is: ")
   fmt.Println(len(greeting))  
}

Hello kloudne!
String Length is: 15

Program exited.

3)package main

import ("fmt" "math" )"fmt" "strings")

func main() {
   greetings :=  []string{"kloud","one!"}   
   fmt.Println(strings.Join(greetings, " "))
}

kloud ne!

4)package main

import (
	"fmt"
	"strings"
)

func main() {
	fmt.Println(strings.Contains("seafood", "fish"))
	fmt.Println(strings.Contains("seafood", "prawns"))
	fmt.Println(strings.Contains("seafood", ""))
	fmt.Println(strings.Contains("", ""))
}


false
false
true
true

Program exited.

5)package main

import (
	"fmt"
	"strings"
)

func main() {
	fmt.Println(strings.Count("cheese", "e"))
	fmt.Println(strings.Count("five", "")) // before & after each rune
}

3
5

Program exited.


6)package main

import (
	"fmt"
	"strings"
)

func main() {
	fmt.Println(strings.EqualFold("Go", "go"))
}

true

Program exited.

7)package main

import (
	"fmt"
	"strings"
)

func main() {
	fmt.Printf("Fields are: %q", strings.Fields("  kloud one  chennai   "))
}

Fields are: ["kloud" "one" "chennai"]
Program exited.

