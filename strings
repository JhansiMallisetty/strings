10)package main

import (
	"fmt"
	"strings"
)

func main() {
	s := []string{"kloud", "one", "chennai"}
	fmt.Println(strings.Join(s, ", "))
}

kloud, one, chennai

Program exited.


11)package main

import (
	"fmt"
	"strings"
)

func main() {
	fmt.Println("ba" + strings.Repeat("na", 2))
}

banana

Program exited.

12)package main

import (
	"fmt"
	"strings"
)

func main() {
	fmt.Println(strings.Replace("kloud kloud kloud", "on", "one", 2))
	fmt.Println(strings.Replace("one one one", "one", "learn", -1))
}

kloud kloud kloud
learn learn learn

Program exited.
